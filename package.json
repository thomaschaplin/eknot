{
    "name": "authy-clone",
    "version": "1.0.0",
    "description": "An authy clone",
    "main": "build/index.js",
    "scripts": {
        "test": "jest",
        "build": "rimraf ./build && tsc",
        "watch": "rimraf ./build && tsc --watch",
        "start": "node build/index.js",
        "lint": "eslint 'src/**/*.ts' --fix",
        "format": "prettier --config .prettierrc . --write",
        "commit": "npx git-cz"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/thomaschaplin/authy-clone"
    },
    "keywords": [
        "typescript",
        "husky",
        "git cz",
        "prettier",
        "eslint",
        "commitlint",
        "lint-staged"
    ],
    "author": "Thomas Chaplin <thomaschaplin@outlook.com>",
    "license": "ISC",
    "devDependencies": {
        "@commitlint/cli": "^8.3.5",
        "@commitlint/config-conventional": "^8.3.4",
        "@types/node": "^13.9.2",
        "@typescript-eslint/eslint-plugin": "^2.24.0",
        "@typescript-eslint/parser": "^2.24.0",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.10.0",
        "eslint-plugin-prettier": "^3.1.2",
        "husky": "^4.2.3",
        "lint-staged": "^10.0.8",
        "prettier": "^2.0.0",
        "rimraf": "^3.0.2",
        "typescript": "^3.8.3"
    },
    "dependencies": {
        "@types/jest": "^26.0.0",
        "jest": "^26.0.1",
        "jimp": "^0.10.3",
        "jsqr": "^1.3.1",
        "otpauth": "^5.0.9"
    }
}
